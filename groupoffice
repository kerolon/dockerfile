FROM ubuntu:16.04
MAINTAINER kerolon
CMD echo -e "\n## Group-Office repository\ndeb http://repos.groupoffice.eu/ sixtwo main" | tee -a /etc/apt/sources.list
RUN apt-get update
RUN apt-get -y install php
RUN apt-get -y install php-fpm
RUN apt-get -y install mysql-server
RUN apt-get -y install nginx
RUN apt-get -y install groupoffice-com
RUN apt-get -y install libwbxml2-utils